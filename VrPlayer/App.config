<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="VrPlayer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <!--General-->
    <add key="CameraFieldOfView" value="110"/>
    <add key="ViewportsHorizontalOffset" value="72"/>
    <add key="ViewportsVerticalOffset" value="0"/>
    <add key="SamplesFolder" value="Samples"/>
    <add key="MetaDataReadOnLoad" value="false"/>
    <!-- Plugins -->
    <add key="DefaultMedia" value="VrPlayer.Medias.VlcDotNet"/>
    <add key="DefaultEffect" value="VrPlayer.Effects.NoEffect"/>
    <add key="DefaultDistortion" value="VrPlayer.Models.Distortions.BarrelEffect"/>
    <add key="DefaultProjection" value="VrPlayer.Projections.Plane"/>
    <add key="DefaultTracker" value="VrPlayer.Models.Trackers.MouseTracker"/>
    <add key="DefaultStabilizer" value="VrPlayer.Models.Stabilizers.Deshaker"/>
  </appSettings>
  <userSettings>
    <VrPlayer.Properties.Settings>
      <setting name="Tracker" serializeAs="String">
        <value>VrPlayer.Models.Trackers.MouseTracker</value>
      </setting>
      <setting name="Distortion" serializeAs="String">
        <value>VrPlayer.Models.Distortions.BarrelEffect</value>
      </setting>
      <setting name="Layout" serializeAs="String">
        <value>SideBySide</value>
      </setting>
    </VrPlayer.Properties.Settings>
  </userSettings>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v2.0" />
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
