	<xmp_schema prefix="VRPlayer" namespace="http://ns.vrplayer.tv/" label="$$$/VRPlayer/Schema/Label=VR Player Properties (http://www.vrplayer.tv)" description="$$$/VRPlayer/Schema/Description=This panel contains the options available for the 'VR Player Panel'.">
	
