<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="VrPlayer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <!--System-->
    <add key="SamplesFolder" value="Medias/Samples/"/>
    <!-- Features -->
    <add key="PositionalAudio" value="false"/>
    <add key="EvrRendering" value="false"/>
    <!--Apparence-->
    <add key="ViewportsHorizontalOffset" value="0"/>
    <add key="CameraFieldOfView" value="90"/>
    <!--Performances-->
    <add key="MouseSensitivity" value="1"/>
    <add key="OrientationRefreshRateInMS" value="10"/>
    <!-- Trackers -->
    <add key="HydraPositionScaleFactor" value="0.002"/>
    <add key="HydraPitchOffset" value="30"/>
    <add key="PsMovePositionScaleFactor" value="0.01"/>
    <add key="KinectPositionScaleFactor" value="1.5"/>
    <!--Effects-->
    <add key="ColorKeyAlphaColor" value="#0000FF"/>
    <add key="ColorKeyTolerance" value="0.5"/>
    <add key="DepthMapMaxOffset" value="0.04"/>
    <!--Distortions-->
    <add key="CustomPincushionFactor" value="5"/>
  </appSettings>
  <userSettings>
    <VrPlayer.Properties.Settings>
      <setting name="Tracker" serializeAs="String">
        <value>VrPlayer.Models.Trackers.MouseTracker</value>
      </setting>
      <setting name="Distortion" serializeAs="String">
        <value>VrPlayer.Models.Shaders.BarrelEffect</value>
      </setting>
      <setting name="Layout" serializeAs="String">
        <value>SideBySide</value>
      </setting>
    </VrPlayer.Properties.Settings>
  </userSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
